{
    "name": "swow/php-stub-generator",
    "description": "PHP extension stub generator",
    "keywords": [
        "swow",
        "extension"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "twose",
            "email": "twosee@php.net"
        }
    ],
    "support": {
        "issues": "https://github.com/swow/php-stub-generator"
    },
    "require": {
        "php": ">=8.0",
        "swow/util": "*"
    },
    "autoload": {
        "psr-4": {
            "Swow\\StubUtils": "src/",
            "Swow\\StubUtils\\ConstantFixer": "src/ConstantFixer"
        }
    },
    "config": {
        "sort-packages": true
    }
}
